<%- include('partials/header') %>

    <div id="content">
        <h2>Search & find roommates <br> in your destination</h2>
        <div id="search">
            <div id="placeholders">
                <p>Search for a city</p>
                <p>Move in date</p>
                <p>Move out date</p>
            </div>
            <form action="/search" method="POST">
                <input type="text" name="search">
                <input type="month" name="moveInDate" placeholder="Move in date" >
                <input type="month" name="moveOutDate" placeholder="Move out date">
            </form>
            <button type="submit">Search</button>
        </div>

<%- include('partials/footer') %>